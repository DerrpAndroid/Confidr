<%= form_for(:user, url: 'newusers' ) do |f| %><%= f.text_field :name, :id=>"home_reg_name",:placeholder=>"John Doe"  %><%= f.text_field :email, :id=>"home_reg_mail",:placeholder=>"john@doe.com"  %><%= f.password_field :password, :id=>"home_reg_password",:placeholder=>"Password"  %><%= f.password_field :password_confirmation, :id=>"home_reg_password",:placeholder=>"Re-Enter Password"  %><%= f.submit "Log in", class: "btn btn-primary" %><% end %>